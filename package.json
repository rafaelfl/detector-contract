{
  "name": "detector-contract",
  "version": "1.0.0",
  "main": "./build/index.js",
  "bin": {
    "detector": "./build/index.js"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "start": "npx ts-node ./src/index.ts",
    "dev": "nodemon src -e ts,json --exec 'yarn start'",
    "test": "jest --watchAll",
    "test-coverage": "jest --watchAll --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write .",
    "build": "yarn clean && tsc -p .",
    "execute": "yarn build && node ./build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.1.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "javascript-fuzzylogic": "^1.3.0",
    "node-fetch": "^3.3.1",
    "uuidv4": "^6.2.13"
  },
  "engines": {
    "node": ">=14.16"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/serviceWorker.ts",
      "!src/config/index.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "resetMocks": false
  }
}
